<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>fireCanary@gerionsecurity.com</em:id>
    <em:version>0.5</em:version>
    <em:type>2</em:type>
    <em:bootstrap>true</em:bootstrap>

    <!-- Target Application this extension can install into, 
         with minimum and maximum supported versions. FIXME--> 
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>10.0</em:minVersion>
        <em:maxVersion>20.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Front End MetaData -->
    <em:name>fireCanary</em:name>
    <em:description>Extendsion to debug JavaScript code in canary mode and trace data through the JavaScript code.</em:description>
    <em:creator>Gergely Revay</em:creator>
    <em:homepageURL>http://gerionsecurity.com</em:homepageURL>
  </Description>
</RDF>
